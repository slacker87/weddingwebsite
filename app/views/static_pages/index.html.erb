<% provide(:title, 'Home')%>

<!-- fancy transition pages -->
<!-- Header with Navigation -->
<div class="row" id="header">
    <div class="col-12">            
        <div id="navigation">
          <ul class="nav navbar-nav">
        <li><a id="link-home" href="#home">Home</a></li>
        <li><a id="link-venue" href="#venue">Venue</a></li>
        <li><a id="link-map" href="#map">Map</a></li>
        <li><a id="link-mens-attire" href="#mens-attire">Mens attire</a></li>
        <li><a id="link-womens-attire" href="#womens-attire">Womens attire</a></li>
        <li><a id="link-photos" href="#photos">Photos</a></li>
          </ul>
      </div>
    </div>
</div>

<!-- Home -->
<div id="home" class="content">
  
</div>
<!-- /Home -->
 

<!-- venue -->
<div id="venue" class="panel">
  <div class="content">
    <div class="row">
      <div class="col-md-10 col-md-offset-1 text-center">
        <row>
        <h2 class="col-xs-12" style="margin-top: -60px;">Venue</h2>
          <p class="col-xs-3" style="background-color: transparent;">The wedding ceremony and reception will take place at the Nassau Inn in Princeton, NJ
          <br/><br/><br/>
          10 Palmer Square, Princeton, NJ 08542
            <br/><br/>
(609) 921-7500
          </p>
          <%= image_tag "nassau.jpg", class: "col-xs-8" %>
        </row>
      </div>
    </div>
  </div>
</div>
<!-- /Venue -->
 
    <!-- Map -->
<div id="map" class="panel">
    <div class="map content">
        <iframe width="100%" height="100%" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?ie=UTF8&amp;q=nassau+inn+princeton&amp;fb=1&amp;gl=us&amp;hq=nassau+inn&amp;cid=0,0,16635396667706573177&amp;ll=40.350396,-74.661281&amp;spn=0.006295,0.006295&amp;t=m&amp;output=embed">
        <br />
        <small>
            <a href="https://maps.google.com/maps?ie=UTF8&amp;q=nassau+inn+princeton&amp;fb=1&amp;gl=us&amp;hq=nassau+inn&amp;cid=0,0,16635396667706573177&amp;ll=40.350396,-74.661281&amp;spn=0.006295,0.006295&amp;t=m&amp;source=embed"></a>
        </small>
        </iframe>
    </div>
</div>
    <!-- /Map -->


<!-- Attire -->
<div id="mens-attire" class="panel services">
    <div class="content">
    <div class="row">
      <div class="col-md-10 col-md-offset-1 text-center">
        <h2>Mens attire</h2>
        <hr>
      </div>
    </div>
    <div class="row">
      <div class="col-md-2 col-md-offset-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-tux"></i>
          <h4>Tuxedo</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-mshoes"></i>
          <h4>Shoes</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-bowtie"></i>
          <h4>Bowtie</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-mextras"></i>
            <h4>Other proper attire</h4>
        </div>
      </div>
    </div>
      </div>
  </div>
  
<div id="womens-attire" class="panel services">
    <div class="content">
    <div class="row">
        <div class="col-md-10 col-md-offset-1 text-center">
          <h2>Womens attire</h2>
          <hr>
        </div>
      </div>
    <div class="row">
      <div class="col-md-2 col-md-offset-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-gown"></i>
          <h4>Formal full-length ball gown</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-makeup"></i>
          <h4>Glamorous make-up</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-jewelry"></i>
          <h4>Dramatic jewelry</h4>
        </div>
      </div>
      <div class="col-md-2 text-center">
        <div class="service-item">
          <i class="service-icon circular-accessories"></i>
            <h4>Other proper accessories</h4>
        </div>
      </div>
    </div>
  </div>
  
</div>
<!-- /attire -->


<!-- Photos -->
<div id="photos" class="panel">
        <div class="content vert-text">
        <h2>Photos</h2>
          <br>
          <%= link_to "Can be found here", photos_path, class: 'btn btn-lg btn-primary' %>
  </div>
</div>
<!-- /photos -->
<!-- /Attire -->
<!-- /transition pages -->

